		<Panel>
			<Text text="Are you sure you want delete this Style?"/>
		</Panel>
		<buttons>
			<Button text="Yes" press="onConfirmDeleteStyle"/>
			<Button text="Cancel" press="onCancelDeleteStyle"/>
