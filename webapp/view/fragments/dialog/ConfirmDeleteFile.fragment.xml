<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Delete File">
		<Panel>
			<Text text="Are you sure you want delete this file?"/>
		</Panel>
		<buttons>
			<Button text="Yes" press="onConfirmDeleteFile"/>
			<Button text="Cancel" press="onCancelDeleteFile"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>