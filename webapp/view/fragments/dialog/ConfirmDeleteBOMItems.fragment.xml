<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Delete BOM Items">
		<Panel>
			<Text text="Are you sure you want delete selected BOM items? Unsaved changes will be lost."/>
		</Panel>
		<buttons>
			<Button text="Yes" press="confirmDeleteBOMItems"/>
			<Button text="Cancel" press="onCancelDeleteBOMItems"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>